{
  "name": "contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy:testnet": "cp .env.testnet .env && source .env && forge script script/CanvasDeploy.s.sol --rpc-url $ZORA_RPC_URL --broadcast",
    "upgrade:testnet": "cp .env.testnet .env && source .env && forge script script/CanvasUpgrade.s.sol --sig 'run(address)' --rpc-url=$ZORA_RPC_URL $(cat broadcast/CanvasDeploy.s.sol/999999999/run-latest.json | jq -r '.transactions[1].contractAddress') --broadcast",
    "deploy:mainnet": "cp .env.mainnet .env && source .env && forge script script/CanvasDeploy.s.sol --rpc-url $ZORA_RPC_URL --broadcast",
    "upgrade:mainnet": "cp .env.mainnet .env && source .env && forge script script/CanvasUpgrade.s.sol --sig 'run(address)' --rpc-url=$ZORA_RPC_URL $(cat broadcast/CanvasDeploy.s.sol/7777777/run-latest.json | jq -r '.transactions[1].contractAddress') --broadcast",
    "test": "pnpm forge test -vv"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@zoralabs/openzeppelin-contracts-upgradeable": "^4.8.4",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#v1.8.1",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.5.2",
    "@openzeppelin/foundry-upgrades": "github:OpenZeppelin/openzeppelin-foundry-upgrades",
    "solidity-stringutils": "github:Arachnid/solidity-stringutils"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@zoralabs/zora-1155-contracts": "^2.7.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "@zoralabs/zora-1155-contracts@2.7.3": "patches/@zoralabs__zora-1155-contracts@2.7.3.patch"
    }
  }
}
